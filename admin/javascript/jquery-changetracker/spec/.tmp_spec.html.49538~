		<script>
			function runSuites() {
				JSpec
				.exec('spec.concrete.basics.js')
				.exec('spec.concrete.selectors.js')
				.exec('spec.concrete.events.js')
				.exec('spec.concrete.has.js')
				.exec('spec.concrete.properties.js')
				.run()
				.report()
			}
		</script>
	</head>
	<body class="jspec" onLoad="runSuites();">
